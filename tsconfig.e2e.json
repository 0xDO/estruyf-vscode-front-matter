{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "outDir": "e2e/out",
    "lib": [
      "es6",
      "dom"
    ],
    "sourceMap": true,
    "strict": false,
    "noUnusedLocals": true,
    "experimentalDecorators": true,
    "resolveJsonModule": true,
    "skipLibCheck": true
  },
  "include": [
    "e2e/src/**/*"
  ],
  "exclude": [
    "node_modules",
    "src"
  ]
}